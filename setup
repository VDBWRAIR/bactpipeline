# Directory setup lives in
THISD=$(cd $(dirname ${BASH_SOURCE[0]}) && pwd)

# Make FLASH if needed
if [ ! -e ${THISD}/dependencies/FLASH*/flash ]
then
    pushd ${THISD}/dependencies/FLASH* > /dev/null
    make
    popd > /dev/null
fi

# Put bin in PATH if not there already
if ! [[ $PATH =~ ${THISD}/bin ]]
then
    export PATH=$PATH:${THISD}/bin
fi
